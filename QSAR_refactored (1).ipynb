    "# Williams plot для baseline-модели Descriptors + Ridge\n",
    "X_train_desc = X_desc_np[train_lit_idx]\n",
    "X_test_desc = X_desc_np[test_exp_idx]\n",
    "y_train_desc = y[train_lit_idx]\n",
    "y_test_desc = y[test_exp_idx]\n",
    "\n",
    "# Трансформации из уже обученной модели (fit на full train_lit)\n",
    "imputer = fitted_desc.named_steps[\"imputer\"]\n",
    "scaler = fitted_desc.named_steps[\"scaler\"]\n",
    "var_threshold = fitted_desc.named_steps[\"var_threshold\"]\n",
    "\n",
    "X_train_proc = var_threshold.transform(scaler.transform(imputer.transform(X_train_desc)))\n",
    "X_test_proc = var_threshold.transform(scaler.transform(imputer.transform(X_test_desc)))\n",
    "\n",
    "# Leverage: h_i = x_i^T (X^T X)^(-1) x_i\n",
    "xtx_inv = np.linalg.pinv(X_train_proc.T @ X_train_proc)\n",
    "leverage_train = np.einsum(\"ij,jk,ik->i\", X_train_proc, xtx_inv, X_train_proc)\n",
    "leverage_test = np.einsum(\"ij,jk,ik->i\", X_test_proc, xtx_inv, X_test_proc)\n",
    "\n",
    "# Остатки и стандартизованные остатки (масштабирование по RMSE train)\n",
    "y_pred_train_desc = fitted_desc.predict(X_train_desc)\n",
    "y_pred_test_desc = fitted_desc.predict(X_test_desc)\n",
    "residual_train = y_train_desc - y_pred_train_desc\n",
    "residual_test = y_test_desc - y_pred_test_desc\n",
    "rmse_train_fit = rmse(y_train_desc, y_pred_train_desc)\n",
    "std_res_train = residual_train / rmse_train_fit\n",
    "std_res_test = residual_test / rmse_train_fit\n",
    "\n",
    "# Williams thresholds\n",
    "n_train = X_train_proc.shape[0]\n",
    "p_features = X_train_proc.shape[1]\n",
    "h_star = 3 * (p_features + 1) / n_train\n",
    "\n",
    "williams_train = pd.DataFrame({\n",
    "    \"set\": \"train_lit\",\n",
    "    \"smiles\": df.loc[train_lit_idx, \"smiles_canon\"].values,\n",
    "    \"y_true\": y_train_desc,\n",
    "    \"y_pred\": y_pred_train_desc,\n",
    "    \"residual\": residual_train,\n",
    "    \"leverage\": leverage_train,\n",
    "    \"std_res\": std_res_train,\n",
    "})\n",
    "\n",
    "williams_test = pd.DataFrame({\n",
    "    \"set\": \"test_exp\",\n",
    "    \"smiles\": df.loc[test_exp_idx, \"smiles_canon\"].values,\n",
    "    \"y_true\": y_test_desc,\n",
    "    \"y_pred\": y_pred_test_desc,\n",
    "    \"residual\": residual_test,\n",
    "    \"leverage\": leverage_test,\n",
    "    \"std_res\": std_res_test,\n",
    "})\n",
    "\n",
    "williams_all = pd.concat([williams_train, williams_test], ignore_index=True)\n",
    "williams_all[\"in_domain\"] = (williams_all[\"leverage\"] <= h_star) & (williams_all[\"std_res\"].abs() <= 3)\n",
    "\n",
    "# Сохраняем predictions_test.csv (только test_exp)\n",
    "predictions_test = williams_all[williams_all[\"set\"] == \"test_exp\"][\n",
    "    [\"smiles\", \"y_true\", \"y_pred\", \"residual\", \"leverage\", \"std_res\", \"in_domain\"]\n",
    "].copy()\n",
    "predictions_test.to_csv(results_dir / \"predictions_test.csv\", index=False)\n",
    "\n",
    "# Williams plot\n",
    "plt.figure(figsize=(7, 6))\n",
    "plt.scatter(\n",
    "    williams_train[\"leverage\"], williams_train[\"std_res\"],\n",
    "    label=\"train_lit\", alpha=0.8, marker=\"o\"\n",
    ")\n",
    "plt.scatter(\n",
    "    williams_test[\"leverage\"], williams_test[\"std_res\"],\n",
    "    label=\"test_exp\", alpha=0.9, marker=\"^\"\n",
    ")\n",
    "plt.axvline(h_star, color=\"red\", linestyle=\"--\", label=f\"h*={h_star:.3f}\")\n",
    "plt.axhline(3, color=\"black\", linestyle=\"--\")\n",
    "plt.axhline(-3, color=\"black\", linestyle=\"--\")\n",
    "plt.xlabel(\"Leverage (h)\")\n",
    "plt.ylabel(\"Standardized residual (residual / RMSE_train)\")\n",
    "plt.title(\"Williams plot: Descriptors + Ridge\")\n",
    "plt.legend()\n",
    "plt.grid(True, alpha=0.3)\n",
    "plt.show()\n",
    "\n",
    "predictions_test.head(10)\n"
    "# Applicability domain на дескрипторах (интерпретируемо)\n",
    "from sklearn.covariance import EmpiricalCovariance\n",
    "\n",
    "X_train_desc = X_desc_np[train_idx]\n",
    "X_test_desc = X_desc_np[test_idx]\n",
    "\n",
    "pipe = Pipeline([\n",
    "    (\"imputer\", SimpleImputer(strategy=\"median\")),\n",
    "    (\"scaler\", StandardScaler()),\n",
    "])\n",
    "Xtr = pipe.fit_transform(X_train_desc)\n",
    "Xte = pipe.transform(X_test_desc)\n",
    "\n",
    "# Махаланобисово расстояние\n",
    "cov = EmpiricalCovariance().fit(Xtr)\n",
    "d_tr = cov.mahalanobis(Xtr)\n",
    "d_te = cov.mahalanobis(Xte)\n",
    "\n",
    "threshold = np.quantile(d_tr, 0.95)\n",
    "ood = d_te > threshold\n",
    "\n",
    "ad_table = pd.DataFrame({\n",
    "    \"smiles\": df.loc[test_idx, \"smiles_canon\"].values,\n",
    "    \"y_obs\": y_test,\n",
    "    \"y_pred\": y_pred,\n",
    "    \"mahalanobis\": d_te,\n",
    "    \"OOD(>95%train)\": ood,\n",
    "}).sort_values(\"mahalanobis\", ascending=False)\n",
    "\n",
    "threshold, ad_table.head(10)\n"
