    "def rmse(y_true, y_pred) -> float:\n",
    "    return float(np.sqrt(mean_squared_error(y_true, y_pred)))\n",
    "\n",
    "def evaluate_with_group_cv(base_model, X_train, y_train, groups_train, X_external=None, y_external=None):\n",
    "    unique_groups = np.unique(groups_train)\n",
    "    n_splits = min(5, len(unique_groups))\n",
    "    if n_splits < 2:\n",
    "        raise ValueError(\"Need at least 2 unique scaffolds in train_lit for GroupKFold CV\")\n",
    "\n",
    "    gkf = GroupKFold(n_splits=n_splits)\n",
    "    oof_pred = np.full_like(y_train, fill_value=np.nan, dtype=float)\n",
    "\n",
    "    for tr, va in gkf.split(X_train, y_train, groups_train):\n",
    "        model = clone(base_model)\n",
    "        model.fit(X_train[tr], y_train[tr])\n",
    "        oof_pred[va] = model.predict(X_train[va])\n",
    "\n",
    "    if np.isnan(oof_pred).any():\n",
    "        raise RuntimeError(\"OOF predictions contain NaN; CV split failed\")\n",
    "\n",
    "    press = float(np.sum((y_train - oof_pred) ** 2))\n",
    "    tss = float(np.sum((y_train - np.mean(y_train)) ** 2))\n",
    "    q2_cv = float(1.0 - press / tss) if tss > 0 else np.nan\n",
    "\n",
    "    cv_metrics = {\n",
    "        \"R2(CV)\": float(r2_score(y_train, oof_pred)),\n",
    "        \"RMSE(CV)\": rmse(y_train, oof_pred),\n",
    "        \"MAE(CV)\": float(mean_absolute_error(y_train, oof_pred)),\n",
    "        \"Q2(CV)\": q2_cv,\n",
    "    }\n",
    "\n",
    "    fitted = clone(base_model)\n",
    "    fitted.fit(X_train, y_train)\n",
    "\n",
    "    external_metrics, pred_external = None, None\n",
    "    if X_external is not None and y_external is not None and len(y_external) > 0:\n",
    "        pred_external = fitted.predict(X_external)\n",
    "        external_metrics = {\n",
    "            \"R2\": float(r2_score(y_external, pred_external)),\n",
    "            \"RMSE\": rmse(y_external, pred_external),\n",
    "            \"MAE\": float(mean_absolute_error(y_external, pred_external)),\n",
    "        }\n",
    "\n",
    "    return cv_metrics, fitted, pred_external, external_metrics\n",
    "\n",
    "def run_protocol(feature_name, model, X_all, y_all, train_idx, test_exp_idx, scaffolds_all, scaf_train_local=None, scaf_test_local=None):\n",
    "    X_train = X_all[train_idx]\n",
    "    y_train = y_all[train_idx]\n",
    "    groups_train = scaffolds_all[train_idx]\n",
    "\n",
    "    X_exp = X_all[test_exp_idx]\n",
    "    y_exp = y_all[test_exp_idx]\n",
    "\n",
    "    cv_metrics, fitted, pred_exp, exp_metrics = evaluate_with_group_cv(\n",
    "        model, X_train, y_train, groups_train, X_external=X_exp, y_external=y_exp\n",
    "    )\n",
    "\n",
    "    model_name = model.named_steps[\"model\"].__class__.__name__ if isinstance(model, Pipeline) else model.__class__.__name__\n",
    "    rows = [{\n",
    "        \"Features\": feature_name,\n",
    "        \"Model\": model_name,\n",
    "    rows = [{\n",
    "        \"Features\": feature_name,\n",
    "        \"Model\": model.named_steps[\"model\"].__class__.__name__ if isinstance(model, Pipeline) else model.__class__.__name__,\n",
    "        \"Split\": \"CV(train_lit)\",\n",
    "        **cv_metrics,\n",
    "        \"R2\": np.nan,\n",
    "        \"RMSE\": np.nan,\n",
    "        \"MAE\": np.nan,\n",
    "    }]\n",
    "\n",
    "    if exp_metrics is not None:\n",
    "        rows.append({\n",
    "            \"Features\": feature_name,\n",
    "            \"Model\": model_name,\n",
    "            \"Model\": rows[0][\"Model\"],\n",
    "            \"Split\": \"external_experimental_test\",\n",
    "            \"R2(CV)\": np.nan,\n",
    "            \"RMSE(CV)\": np.nan,\n",
    "            \"MAE(CV)\": np.nan,\n",
    "            \"Q2(CV)\": np.nan,\n",
    "            **exp_metrics,\n",
    "        })\n",
    "\n",
    "    pred_scaf = None\n",
    "    if scaf_train_local is not None and scaf_test_local is not None and len(scaf_test_local) > 0 and len(scaf_train_local) > 0:\n",
    "        model_scaf = clone(model)\n",
    "        model_scaf.fit(X_train[scaf_train_local], y_train[scaf_train_local])\n",
    "        pred_scaf = model_scaf.predict(X_train[scaf_test_local])\n",
    "        rows.append({\n",
    "            \"Features\": feature_name,\n",
    "            \"Model\": model_name,\n",
    "            \"Model\": rows[0][\"Model\"],\n",
    "            \"Split\": \"external_test_scaffold\",\n",
    "            \"R2(CV)\": np.nan,\n",
    "            \"RMSE(CV)\": np.nan,\n",
    "            \"MAE(CV)\": np.nan,\n",
    "            \"Q2(CV)\": np.nan,\n",
    "            \"R2\": float(r2_score(y_train[scaf_test_local], pred_scaf)),\n",
    "            \"RMSE\": rmse(y_train[scaf_test_local], pred_scaf),\n",
    "            \"MAE\": float(mean_absolute_error(y_train[scaf_test_local], pred_scaf)),\n",
    "        })\n",
    "\n",
    "    pred_exp_df = pd.DataFrame({\n",
    "        \"row_index\": test_exp_idx,\n",
    "        \"smiles\": df.loc[test_exp_idx, \"smiles_canon\"].values,\n",
    "        \"y_true\": y_exp,\n",
    "        \"y_pred\": pred_exp,\n",
    "    })\n",
    "    pred_exp_df[\"residual\"] = pred_exp_df[\"y_true\"] - pred_exp_df[\"y_pred\"]\n",
    "\n",
    "    return pd.DataFrame(rows), pred_exp_df, fitted\n",
    "\n",
    "ridge_desc = Pipeline([\n",
    "    (\"imputer\", SimpleImputer(strategy=\"median\")),\n",
    "    (\"scaler\", StandardScaler()),\n",
    "    (\"var_threshold\", VarianceThreshold(threshold=0.0)),\n",
    "    (\"model\", Ridge(alpha=1.0, random_state=0)),\n",
    "])\n",
    "\n",
    "rf_fp = RandomForestRegressor(\n",
    "    n_estimators=500, random_state=0, n_jobs=-1, min_samples_leaf=2\n",
    ")\n",
    "\n",
    "X_desc_np = X_desc.to_numpy(dtype=float)\n",
    "X_fp_np = X_fp.astype(float)\n",
    "\n",
    "summary_desc, pred_desc_exp, fitted_desc = run_protocol(\n",
    "    \"Descriptors\", ridge_desc, X_desc_np, y, train_lit_idx, test_exp_idx, scaffolds,\n",
    "    scaf_train_local=scaf_train_local if has_external_scaffold else None,\n",
    "    scaf_test_local=scaf_test_local if has_external_scaffold else None,\n",
    ")\n",
    "summary_fp, pred_fp_exp, fitted_fp = run_protocol(\n",
    "    \"MorganFP\", rf_fp, X_fp_np, y, train_lit_idx, test_exp_idx, scaffolds,\n",
    "    scaf_train_local=scaf_train_local if has_external_scaffold else None,\n",
    "    scaf_test_local=scaf_test_local if has_external_scaffold else None,\n",
    ")\n",
    "\n",
    "results_dir = Path(\"artifacts/results\")\n",
    "results_dir.mkdir(parents=True, exist_ok=True)\n",
    "\n",
    "metrics_summary = pd.concat([summary_desc, summary_fp], ignore_index=True)\n",
    "metrics_summary.to_csv(results_dir / \"metrics_summary.csv\", index=False)\n",
    "\n",
    "# Экспортируем только строки 90+ (experimental holdout) для MorganFP+RandomForest\n",
    "predictions_experimental = pred_fp_exp.copy()\n",
    "predictions_experimental.to_csv(results_dir / \"predictions_experimental.csv\", index=False)\n",
    "metrics_summary = pd.concat([summary_desc, summary_fp], ignore_index=True)\n",
    "metrics_summary.to_csv(\"metrics_summary.csv\", index=False)\n",
    "\n",
    "# Экспортируем только строки 90+ (experimental holdout) для MorganFP+RandomForest\n",
    "predictions_experimental = pred_fp_exp.copy()\n",
    "predictions_experimental.to_csv(\"predictions_experimental.csv\", index=False)\n",
    "\n",
    "metrics_summary\n"
   ],
