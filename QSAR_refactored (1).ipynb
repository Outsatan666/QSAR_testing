    "from collections import defaultdict\n",
    "\n",
    "ORDER_CANDIDATES = [\"№\", \"No\", \"no\", \"N\", \"n\", \"index\", \"Index\"]\n",
    "order_col = next((c for c in ORDER_CANDIDATES if c in df.columns), None)\n",
    "\n",
    "if order_col is not None:\n",
    "    order_vals = pd.to_numeric(df[order_col], errors=\"coerce\")\n",
    "    if order_vals.notna().all():\n",
    "        # ожидаем 1-based нумерацию, поэтому 1..90 -> train_lit\n",
    "        in_train_lit = order_vals <= 90\n",
    "        split_source = f\"column '{order_col}'\"\n",
    "    else:\n",
    "        in_train_lit = df.index < 90\n",
    "        split_source = \"row index fallback (non-numeric order column)\"\n",
    "else:\n",
    "    in_train_lit = df.index < 90\n",
    "    split_source = \"row index fallback\"\n",
    "\n",
    "train_lit_idx = np.where(in_train_lit.to_numpy())[0]\n",
    "test_exp_idx = np.where((~in_train_lit).to_numpy())[0]\n",
    "\n",
    "if len(test_exp_idx) == 0:\n",
    "    raise ValueError(\"External experimental set is empty. Expected rows 90+ in potok.csv\")\n",
    "\n",
    "print(f\"Split source: {split_source}\")\n",
    "print(f\"train_lit size: {len(train_lit_idx)}\")\n",
    "print(f\"test_exp size: {len(test_exp_idx)}\")\n",
    "\n",
    "def murcko_scaffold(smiles: str) -> str:\n",
    "    mol = Chem.MolFromSmiles(smiles)\n",
    "    if mol is None:\n",
    "        return \"\"\n",
    "    scaf = MurckoScaffold.GetScaffoldForMol(mol)\n",
    "    return Chem.MolToSmiles(scaf, canonical=True)\n",
    "\n",
    "scaffolds = np.array([murcko_scaffold(s) for s in smis])\n",
    "scaffolds_train_lit = scaffolds[train_lit_idx]\n",
    "pd.Series(scaffolds_train_lit).value_counts().head(10)\n"
