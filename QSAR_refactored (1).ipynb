    "def scaffold_split_indices(scaffold_labels: np.ndarray, test_size: float = 0.2, random_state: int = 42):\n",
    "    \"\"\"Optional scaffold holdout *inside train_lit* for an additional external-like check.\"\"\"\n",
    "    rng = np.random.default_rng(random_state)\n",
    "    groups = defaultdict(list)\n",
    "    for i, sc in enumerate(scaffold_labels):\n",
    "        groups[sc].append(i)\n",
    "\n",
    "    items = list(groups.items())\n",
    "    rng.shuffle(items)\n",
    "    items.sort(key=lambda x: len(x[1]), reverse=True)\n",
    "\n",
    "    n_total = len(scaffold_labels)\n",
    "    n_test_target = max(1, int(round(n_total * test_size)))\n",
    "\n",
    "    test_local, train_local = [], []\n",
    "    for _, idxs in items:\n",
    "        if len(test_local) + len(idxs) <= n_test_target:\n",
    "            test_local.extend(idxs)\n",
    "        else:\n",
    "            train_local.extend(idxs)\n",
    "\n",
    "    if len(test_local) == 0 and len(train_local) > 1:\n",
    "        rng.shuffle(train_local)\n",
    "        test_local = train_local[:1]\n",
    "        train_local = train_local[1:]\n",
    "\n",
    "    return np.array(sorted(train_local), dtype=int), np.array(sorted(test_local), dtype=int)\n",
    "\n",
    "scaf_train_local, scaf_test_local = scaffold_split_indices(scaffolds_train_lit, test_size=0.2, random_state=42)\n",
    "has_external_scaffold = len(scaf_test_local) > 0 and len(scaf_train_local) > 0\n",
    "print(\"external_test_scaffold inside train_lit:\", has_external_scaffold, \"size:\", len(scaf_test_local))\n"
   ],
   "id": "gLkL-gcmKdV7"
