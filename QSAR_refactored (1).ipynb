   ],
   "source": [
    "# Минимальная валидация\n",
    "required_cols = {\"smiles\", \"activity\"}\n",
    "missing = required_cols - set(df.columns)\n",
    "if missing:\n",
    "    raise ValueError(f\"Нет колонок: {missing}. Найдены: {list(df.columns)}\")\n",
    "\n",
    "df = df.copy()\n",
    "df[\"activity\"] = pd.to_numeric(df[\"activity\"], errors=\"coerce\")\n",
    "\n",
    "# Удаляем строки с NaN активности\n",
    "df = df.dropna(subset=[\"activity\"]).reset_index(drop=True)\n",
    "\n",
    "# Валидируем SMILES\n",
    "def canon_smiles(smi: str) -> str | None:\n",
    "    mol = Chem.MolFromSmiles(str(smi))\n",
    "    if mol is None:\n",
    "        return None\n",
    "    return Chem.MolToSmiles(mol, canonical=True)\n",
    "\n",
    "df[\"smiles_canon\"] = df[\"smiles\"].map(canon_smiles)\n",
    "bad = df[\"smiles_canon\"].isna().sum()\n",
    "print(\"Невалидных SMILES:\", bad)\n",
    "\n",
    "df = df.dropna(subset=[\"smiles_canon\"]).reset_index(drop=True)\n",
    "df.head(), df.shape\n"
   ],
   "id": "XepkkWNGKdV5"
  },
