    "DESCRIPTOR_FUNCS = {\n",
    "    \"MolWt\": Descriptors.MolWt,\n",
    "    \"MolLogP\": Descriptors.MolLogP,\n",
    "    \"TPSA\": Descriptors.TPSA,\n",
    "    \"HBD\": Descriptors.NumHDonors,\n",
    "    \"HBA\": Descriptors.NumHAcceptors,\n",
    "    \"RotBonds\": Descriptors.NumRotatableBonds,\n",
    "    \"Heteroatoms\": Descriptors.NumHeteroatoms,\n",
    "    \"RingCount\": Descriptors.RingCount,\n",
    "    \"AromaticRings\": Descriptors.NumAromaticRings,\n",
    "    \"FracCSP3\": Descriptors.FractionCSP3,\n",
    "}\n",
    "\n",
    "def compute_descriptors(smiles_list: list[str]) -> pd.DataFrame:\n",
    "    rows = []\n",
    "    for smi in smiles_list:\n",
    "        mol = Chem.MolFromSmiles(smi)\n",
    "        if mol is None:\n",
    "            rows.append({k: np.nan for k in DESCRIPTOR_FUNCS})\n",
    "            continue\n",
    "        rows.append({k: float(fn(mol)) for k, fn in DESCRIPTOR_FUNCS.items()})\n",
    "    return pd.DataFrame(rows)\n",
    "\n",
    "def compute_morgan_bits(smiles_list: list[str], radius: int = 2, n_bits: int = 2048) -> np.ndarray:\n",
    "    fps = np.zeros((len(smiles_list), n_bits), dtype=np.uint8)\n",
    "    for i, smi in enumerate(smiles_list):\n",
    "        mol = Chem.MolFromSmiles(smi)\n",
    "        if mol is None:\n",
    "            continue\n",
    "        bv = AllChem.GetMorganFingerprintAsBitVect(mol, radius, nBits=n_bits)\n",
    "        arr = np.zeros((n_bits,), dtype=np.int8)\n",
    "        Chem.DataStructs.ConvertToNumpyArray(bv, arr)\n",
    "        fps[i, :] = arr\n",
    "    return fps\n",
    "\n",
    "smis = df[\"smiles_canon\"].tolist()\n",
    "\n",
    "X_desc = compute_descriptors(smis)\n",
    "X_fp = compute_morgan_bits(smis, radius=2, n_bits=2048)\n",
    "\n",
    "print(\"Descriptors:\", X_desc.shape)\n",
    "print(\"Fingerprints:\", X_fp.shape)\n",
    "X_desc.head()\n"
